{
  "name": "react-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "api": "php -S localhost:8088 -t public/api",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint \"app/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"app/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:fix": "lint-staged",
    "deps": "npx npm-check --skip-unused -u",
    "update-deps": "npx npm-check-updates -u",
    "prepare": "husky",
    "local": "concurrently --names \"FRONTEND,API\" --prefix-colors \"blue,green\" \"npm run dev\" \"npm run api\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-router/node": "^7.6.1",
    "@react-router/serve": "^7.6.1",
    "@tanstack/react-query": "^5.79.0",
    "axios": "^1.9.0",
    "eslint-plugin-prettier": "^5.4.1",
    "isbot": "^5.1.28",
    "motion": "^12.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router": "^7.6.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@react-router/dev": "^7.6.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@types/dompurify": "^3.2.0",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "concurrently": "^9.1.2",
    "dompurify": "^3.2.6",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
